                "inject": false
              },
              {
                "input": "public/themes/bootstrap4-light-purple.css",
                "bundleName": "bootstrap4-light-purple",
                "inject": false
              },
              {
                "input": "public/themes/fluent-light.css",
                "bundleName": "fluent-light",
                "inject": false
              },
              {
                "input": "public/themes/lara-dark-blue.css",
                "bundleName": "lara-dark-blue",
                "inject": false
              },
              {
                "input": "public/themes/lara-dark-indigo.css",
                "bundleName": "lara-dark-indigo",
                "inject": false
              },
              {
                "input": "public/themes/lara-dark-purple.css",
                "bundleName": "lara-dark-purple",
                "inject": false
              },
              {
                "input": "public/themes/lara-dark-teal.css",
                "bundleName": "lara-dark-teal",
