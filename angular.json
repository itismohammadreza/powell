{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "powell": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/powell",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets",
              "src/env.js",
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              {
                "input": "public/themes/arya-blue.css",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "public/themes/arya-green.css",
                "bundleName": "arya-green",
                "inject": false
              },
              {
                "input": "public/themes/arya-orange.css",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "public/themes/arya-purple.css",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "public/themes/bootstrap4-dark-blue.css",
                "bundleName": "bootstrap4-dark-blue",
                "inject": false
              },
              {
                "input": "public/themes/bootstrap4-dark-purple.css",
                "bundleName": "bootstrap4-dark-purple",
                "inject": false
              },
              {
                "input": "public/themes/bootstrap4-light-blue.css",
